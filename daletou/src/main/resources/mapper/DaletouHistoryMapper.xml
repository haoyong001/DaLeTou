<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ball.ball.dao.DaletouHistoryDao">

    <resultMap id="BaseResultMap" type="com.ball.ball.entity.DaletouHistory">
        <id column="issue_no" property="issueNo" jdbcType="INTEGER"/>
        <result column="red_one" property="redOne" jdbcType="VARCHAR"/>
        <result column="red_two" property="redTwo" jdbcType="VARCHAR"/>
        <result column="red_three" property="redThree" jdbcType="VARCHAR"/>
        <result column="red_four" property="redFour" jdbcType="VARCHAR"/>
        <result column="red_five" property="redFive" jdbcType="VARCHAR"/>
        <result column="blue_one" property="blueOne" jdbcType="VARCHAR"/>
        <result column="blue_two" property="blueTwo" jdbcType="VARCHAR"/>
        <result column="date_time" property="dateTime" jdbcType="VARCHAR"/>
    </resultMap>

    <select id="findAll" resultMap="BaseResultMap">
        select * from daletou_history
    </select>

    <insert id="batchSave" parameterType="DaletouHistory" useGeneratedKeys="true" keyProperty="issueNo">
        insert into daletou_history values
        <foreach collection="list" item="w" separator="," close=";">
            (#{w.issueNo},#{w.redOne},#{w.redTwo},#{w.redThree},#{w.redFour},#{w.redFive},#{w.blueOne},#{w.blueTwo},#{w.dateTime})
        </foreach>
    </insert>

</mapper>